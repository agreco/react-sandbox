{
  "name": "react-sandbox with gulp, webpack and mocha chai",
  "version": "1.0.0",
  "description": "React Sandbox with Gulp, Webpack and Mocha Chai",
  "private": true,
  "main": "src/server/index.js",
  "engines": {
    "node": "0.10.35"
