    "gulp-jest": "^0.4.0",
    "gulp-jsx": "2.0.0",
    "gulp-mocha": "2.1.3",
    "gulp-open": "1.0.0",
    "gulp-react": "^3.0.1",
    "gulp-uglify": "1.2.0",
    "gulp-util": "^2.2.19",
    "gulp-webpack": "^1.1.2",
    "jest": "*",
    "jest-cli": "*",
    "jquery": "*",
    "jsdom": "^2.0.0",
    "jsx-loader": "*",
    "mocha": "2.2.5",
    "nock": "2.6.0",
    "react": "*",
    "react-tools": "*",
    "socket.io": "^1.3.6",
    "webpack": "^1.10.5",
    "xmlhttprequest": "^1.7.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/test/preprocessor.js",
